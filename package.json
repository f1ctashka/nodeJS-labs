{
  "name": "nodejs-labs",
  "version": "1.0.0",
  "description": "Node.js Labs s. 5",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index",
    "format": "prettier --check \"src/**/*{.js,.ts}\" --ignore-path .gitignore",
    "format:fix": "prettier --write \"src/**/*{.js,.ts}\" --ignore-path .gitignore",
    "lint": "eslint --ignore-path .gitignore --cache \"src/**/*{.js,.ts}\"",
    "lint:fix": "eslint --ignore-path .gitignore --cache \"src/**/*{.js,.ts}\" --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f1ctashka/nodeJS-labs.git"
  },
  "keywords": [
    "Node.js",
    "KPI"
  ],
  "author": "Victoria Ilchenko IK-01, Vadym Stroy IK-02",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/f1ctashka/nodeJS-labs/issues"
  },
  "homepage": "https://github.com/f1ctashka/nodeJS-labs#readme",
  "private": true,
  "devDependencies": {
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.16.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1"
  }
}
